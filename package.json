{
  "name": "todo-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/* --fix",
    "lint:fix": "eslint '**/*.ts' --fix",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register -r dotenv-flow/config src/index.ts",
    "build:test": "npm run lint && webpack --config webpack.config.dev.js",
    "start:test": "cross-env NODE_ENV=test node -r dotenv-flow/config dist/index.js",
    "build:production": "npm run lint && webpack --config webpack.config.js",
    "start:production": "cross-env NODE_ENV=production node -r dotenv-flow/config dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv-flow": "^3.1.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cross-env": "^7.0.2",
    "dotenv-flow-webpack": "^1.1.0",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
